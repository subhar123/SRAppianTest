<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0e6-ca6a-8000-6ee0-010000010000_70141</versionUuid>
    <rule>
        <name>EPAS_applyComponentsForUser</name>
        <uuid>_a-0000dd0c-ffa4-8000-5130-010000010000_42739</uuid>
        <description>Apply components for users</description>
        <parentUuid>_a-0000dd0c-ffa4-8000-5130-010000010000_42722</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!userId:ri!columnValues[ri!userIndex],
  local!responseInfo:index(ri!responseInfo,wherecontains(ri!questionName,index(ri!responseInfo,"questionName",{}))),
  #"SYSTEM_SYSRULES_textField"(
    label:"User",
    value:index(local!responseInfo,wherecontains(local!userId,index(local!responseInfo,"reviewerId",{}))).response,
    readOnly:true()
  )
)</definition>
        <namedTypedValue>
            <name>userIndex</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>questionName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>responseInfo</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>columnValues</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
