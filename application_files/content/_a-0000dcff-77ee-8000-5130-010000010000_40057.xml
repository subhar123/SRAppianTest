<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0e6-ca6a-8000-6ee0-010000010000_69992</versionUuid>
    <rule>
        <name>EPAS_populateAdjudDocCdt</name>
        <uuid>_a-0000dcff-77ee-8000-5130-010000010000_40057</uuid>
        <description>Expression Rule to Populated Adjudication Document CDT</description>
        <parentUuid>_a-0000dd0c-ffa4-8000-5130-010000010000_42721</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!file: index(ri!files,ri!index,null),
  local!fileName: document(tointeger(local!file),"name")&amp;"."&amp;document(tointeger(local!file),"extension"),
  local!docType: index(ri!docType,ri!index,null),
  local!docName: index(ri!docName,ri!index,null),
  
  
  {
    docName: local!fileName,
    docType: if(local!docType = "Other", local!docType&amp;" - "&amp;local!docName, local!docType),
    docId: local!file,
    uploadedUserId: ri!user
    
  }
  
)</definition>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>files</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>docType</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>docName</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>user</name>
            <type>
                <name>User</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
