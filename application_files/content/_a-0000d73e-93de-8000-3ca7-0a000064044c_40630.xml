<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0e6-ca6a-8000-6ee0-010000010000_70129</versionUuid>
    <rule>
        <name>shortenUserNameForTempoReport</name>
        <uuid>_a-0000d73e-93de-8000-3ca7-0a000064044c_40630</uuid>
        <description>If you have long username formats, create a rule to shorten them generically. The default is to take everything before the @.</description>
        <parentUuid>_a-0000d72c-f160-8000-ecfa-010000010000_1946</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=split(ri!un,"@")[1]</definition>
        <namedTypedValue>
            <name>un</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
