    <versionUuid>_a-0000e0e6-ca6a-8000-6ee0-010000010000_70080</versionUuid>
    <rule>
        <name>EPAS_formatDateTimeForDashboard</name>
        <uuid>_a-0000dd0c-ffa4-8000-5130-010000010000_41918</uuid>
        <description>Format Date Time</description>
        <parentUuid>_a-0000dd0c-ffa4-8000-5130-010000010000_42722</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=if(isnull(ri!dt),ri!dt,text(ri!dt,"mmm-dd-yyyy hh:mm a"))</definition>
        <namedTypedValue>
            <name>dt</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
